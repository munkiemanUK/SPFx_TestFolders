{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "test-folders-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/testFolders/TestFoldersWebPart.js",
          "manifest": "./src/webparts/testFolders/TestFoldersWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
